{
  "name": "can-data-context",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "system": {
    "npmAlgorithm": "flat",
    "npmIgnore": [
      "testee"
    ],
    "bundlesPath": "./target",
    "main": "index",
    "directories": {
      "lib": "src"
    },
    "ext": {
      "txt": "systemjs-plugin-text"
    },
    "plugins": [
      "steal-less"
    ]
  },
  "devDependencies": {
    "can-fixture": "^1.0.12",
    "jasmine": "^2.5.3",
    "steal": "^1.1.0",
    "steal-jasmine": "^0.0.3",
    "steal-tools": "^1.1.1",
    "testee": "^0.3.0",
    "xo": "^0.17.1"
  },
  "dependencies": {
    "can": "^3.4.1",
    "can-component": "^3.0.5",
    "can-compute": "^3.0.5",
    "can-connect": "^1.3.0",
    "can-define": "^1.0.16",
    "can-event": "^3.0.2",
    "can-route": "^3.0.6",
    "can-route-pushstate": "^3.0.1",
    "can-set": "^1.0.3",
    "can-stache": "^3.0.19",
    "can-stache-bindings": "^3.0.6",
    "clone": "^2.1.0",
    "data.either": "^1.5.0",
    "data.maybe": "^1.2.2",
    "steal-less": "^1.1.0"
  },
  "xo": {
    "envs": [
      "browser",
      "jasmine"
    ],
    "space": 4,
    "rules": {
      "one-var": [
        0,
        "never"
      ],
      "quote-props": [
        0,
        "consistent"
      ],
      "no-mixed-requires": 0,
      "space-before-function-paren": 0
    }
  }
}
