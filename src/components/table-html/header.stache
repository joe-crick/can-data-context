<thead>
    {{#each displayHeadings}}
        <tr {{#if className}}class="{{className}}"{{/if}}>
            {{#each cells}}
                <th {{#if headerClasses}}class="{{headerClassName()}}"{{/if}}
                    {{#if rowspan}}rowspan="{{rowspan}}"{{/if}}
                    {{#if colspan}}colspan="{{colspan}}"{{/if}}>
                    {{{heading}}}
                </th>
            {{/each}}
        </tr>
    {{/each}}
</thead>