<can-import from="can-table/components/pagination/"/>
<can-import from="can-table/components/table-html/"/>

<can-table-pagination
        class="pagination"
        {lower-bounds}="lowerBounds"
        {rows-per-page}="rowsPerPage"
        {row-count}="rowCount"
        {^table-filters}="@tableFilters"/>

<can-table-html
        class="can-table"
        {headings}="headings"
        {rows}="rows"
        {table-classes}="tableClasses"
        {table-filters}="@tableFilters"/>
